{
  "schemaVersion": 1,
  "id": "ra_additions_goals",
  "version": "${version}",
  "name": "RAA Goals",
  "description": "An RAA module that allows for the modification, creation, and removal, of behaviours from minecraft mobs.",
  "authors": [ "ThatRobin" ],
  "contact": {
    "repo": "https://github.com/robin/ra_additions_goals"
  },
  "license": "MIT",
  "icon": "assets/ra_additions/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.thatrobin.ra_additions_goals.client.RAA_Goals_Client"
    ],
    "main": [
      "io.github.thatrobin.ra_additions_goals.RAA_Goals"
    ]
  },
  "custom": {
    "modmenu": {
      "parent": "ra_additions",
      "badges": [
        "library"
      ]
    }
  },
  "mixins": [
    "ra_additions_goals.mixins.json"
  ],
  "accessWidener": "raa_goals.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "1.20.x"
  }
