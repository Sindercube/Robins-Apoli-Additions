{
  "schemaVersion": 1,
  "id": "ra_additions_choices",
  "version": "${version}",
  "name": "RAA Choices",
  "description": "An RAA module that adds a choice system into Apoli, giving devs the ability to grant powers based on choices made.",
  "authors": [ "ThatRobin" ],
  "contact": {
    "repo": "https://github.com/robin/ra_additions_choices"
  },
  "license": "MIT",
  "icon": "assets/ra_additions/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.thatrobin.ra_additions_choices.client.RAA_Choices_Client"
    ],
    "main": [
      "io.github.thatrobin.ra_additions_choices.RAA_Choices"
    ],
    "cardinal-components-entity": [
      "io.github.thatrobin.ra_additions_choices.component.ModComponents"
    ],
    "calio:ordered-resource-listener": [
      "io.github.thatrobin.ra_additions_choices.RAA_Choices"
    ]
  },
  "custom": {
    "cardinal-components": [
      "ra_additions:choice"
    ],
    "modmenu": {
      "parent": "ra_additions",
      "badges": [
        "library"
      ]
    }
  },
  "mixins": [
    "ra_additions_choices.mixins.json"
  ],
  "accessWidener": "raa_choices.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "1.20.x"
  }
}
